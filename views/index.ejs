<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
    <meta charset="utf-8">
    <title>Magellan Scraper</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/table_stylesheets/style.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/table_stylesheets/util.css">

    <!-- Used Library: https://colorlib.com/wp/template/fixed-header-table/-->
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/vendor/perfect-scrollbar/perfect-scrollbar.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/stylesheets/ownStyles.css">
</head>

<body>
    <div id = "title-container">
        <div id="title">MAGELLAN.INFO</div>
    </div>
    
    <div class="limiter">
        <div class="container-table100 container-fluid">
            <div class="wrap-table100">
                <div class="table100 ver1 m-b-20">
                    <div class="table100-head">
                        <table>
                            <thead>
                                <tr class="row100 head">
                                    <th class="cell100 column1">Course</th>
                                    <th class="cell100 column2">Description</th>
                                    <th class="cell100 column3">Math</th>
                                    <th class="cell100 column4">NS</th>
                                    <th class="cell100 column5">CS</th>
                                    <th class="cell100 column6">ES</th>
                                    <th class="cell100 column7">ED</th>
                                    <th class="cell100 column8">Spots</th>
                                </tr>
                            </thead>
                        </table>
                    </div>

                    <div class="table100-body js-pscroll">
                        <table>
                            <tbody>
                                <!-- <% listOfCourses.forEach(function(course) { %>
                                    <tr class="row100 body">
                                        <td class="cell100 column1">
                                            <%= course.Code %>
                                        </td>
                                        <td class="cell100 column2">
                                            <%= course.Title %>
                                        </td>
                                        <td class="cell100 column3">
                                            <%= course.Math %>
                                        </td>
                                        <td class="cell100 column4">
                                            <%= course.NS %>
                                        </td>
                                        <td class="cell100 column5">
                                            <%= course.CS %>
                                        </td>
                                        <td class="cell100 column6">
                                            <%= course.ES %>
                                        </td>
                                        <td class="cell100 column7">
                                            <%= course.ED %>
                                        </td>
                                        <td class="cell100 column8">
                                            <button type="button" class="btn btn-danger" onclick="removeCourse()">Remove</button>
                                        </td>
                                    </tr>
                                    <% }); %> -->
                                        <tr class="row100 body" id="initial">
                                            <td class="cell100 column1">Initial Credits</td>
                                            <td class="cell100 column2"></td>
                                            <td class="cell100 column3"></td>
                                            <td class="cell100 column4"></td>
                                            <td class="cell100 column5"></td>
                                            <td class="cell100 column6"></td>
                                            <td class="cell100 column7"></td>
                                            <td class="cell100 column8"></td>
                                        </tr>

                                        <tr class="row100 body" id="footer">
                                            <td class="cell100 column1">Total</td>
                                            <td class="cell100 column2"></td>
                                            <td class="cell100 column3">0</td>
                                            <td class="cell100 column4">0</td>
                                            <td class="cell100 column5">0</td>
                                            <td class="cell100 column6">0</td>
                                            <td class="cell100 column7">0</td>
                                            <td class="cell100 column8"></td>
                                        </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="button-container" style='padding-top: 0px;'>
                    <button type="button" class="btn btn-success" style="width: 50%" onclick="showForm()">Add New Class</button>
                    <button type="button" class="btn btn-success" style="width: 30%" onclick="promptInitialAU()" id="mod">Mod Initial AU Units</button>
                </div>

                <div class="btn-group download-container">
                    <button type="file" id="fileForUpload" class="btn btn-primary" onclick="triggerJSON()">
                        Load Existing JSON</button>
                        <button type="button" class="btn btn-primary" onclick="downloadJSON()">Download</button>
                </div>

                <form style="visibility:hidden">
                    <input id="myInput" type="file" style="visibility:hidden" onchange="loadJSON()" />
                    <input type="submit" value="Upload Image" name="submit"/>
                </form>

            </div>
        </div>
    </div>

    <!-- limiter -->

</body>

<!--===============================================================================================-->
<script src="/vendor/jquery/jquery-3.2.1.min.js"></script>
